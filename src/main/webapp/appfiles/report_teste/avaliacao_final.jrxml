	<queryString>
		<![CDATA[SELECT 
	av.nome,
	av.cargo_nome,
	av.numfunc,
	av.numvinc,
	f.quadro,
	av.setor_nome,
	av.regional_nome,
	av.etapa,
	f.ocupantes
FROM aede.avaliacao as av
	INNER JOIN aede.formulario as f ON av.formulario = f.id
WHERE
	f.excluido = 'N' AND
	f.ativo = true AND
	f.id = $P{id_formulario} AND 
	(av.status = 'CRIADA COMISSAO' OR av.status = 'RECURSO')
	$P!{RETORNO}]]>
	</queryString>
