	<queryString>
		<![CDATA[SELECT (SELECT AVG(nota) FROM aede.tabulacao as ta WHERE ta.avaliacao = $P!{idavaliacao}) as media,f.nome as formulario,a.id,a.nome_servidor, a.cargo, a.quadro,a.orgao,a.setor,a.regional, a.num_func, a.num_vinc, a.nota,a.etapa,a.dt_ini_etapa,dt_fim_etapa 
FROM aede.avaliacao AS a , aede.formulario f 
WHERE a.status = 'TABULADA' 
      AND a.id= $P!{idavaliacao}
      AND a.formulario= f.id]]>
	</queryString>
